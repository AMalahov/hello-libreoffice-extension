<?xml version="1.0" encoding="UTF-8"?>
<description xmlns="http://openoffice.org/extensions/description/2006"
             xmlns:d="http://openoffice.org/extensions/description/2006"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <version value="1.0"/>

    <identifier value="com.github.amalahov.hellooffice.n14" />

    <dependencies>
        <OpenOffice.org-minimal-version value="4.1" d:name="OpenOffice.org 4.1"/>
    </dependencies>

    <registration>
        <simple-license accept-by="admin" default-license-id="this" suppress-on-update="true" suppress-if-required="true">
            <license-text xlink:href="LICENSE.txt" lang="en" license-id="this" />
        </simple-license>
    </registration>

    <display-name>
        <name lang="en">LibreOffice extension example</name>
        <name lang="ru">Пример расширения LibreOffice</name>
    </display-name>

    <extension-description>
        <src lang="en" xlink:href="user-extension-description/descr.en.txt"/>
        <src lang="ru" xlink:href="user-extension-description/descr.ru.txt"/>
    </extension-description>

</description>

